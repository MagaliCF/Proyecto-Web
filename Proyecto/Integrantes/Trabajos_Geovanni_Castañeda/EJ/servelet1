public class Holamundo extends HyypServlet{
public void doGet(
  HttpServletRequest request,
  HttpServletResponse response
)throws Serve√±tException, IOException
{
  response.setContentType("text/html");
  PrintWriter out= response.getWriter();
  String docType= "<!Doctype html public \"-//W3XC//DTD html 4.0 "+"Transitional //EN\">\n";
  out.println(docType+"<html>\n"+"<head><title>Hola mundo</title></head>\n"+"<body>\n"+"<h1>Hola Mundo</h1>\n"+"</body></html>");
}
}
